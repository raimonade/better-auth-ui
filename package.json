{
    "name": "@daveyplate/better-auth-ui",
    "homepage": "https://better-auth-ui.com",
    "version": "2.0.15",
    "description": "Plug & play shadcn/ui components for better-auth",
    "scripts": {
        "build": "tsup --dts",
        "dev": "tsc-watch",
        "prepublishOnly": "rm -rf dist && turbo build"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./server": {
            "import": {
                "types": "./dist/server.d.ts",
                "default": "./dist/server.js"
            },
            "require": {
                "types": "./dist/server.d.cts",
                "default": "./dist/server.cjs"
            }
        },
        "./tanstack": {
            "import": {
                "types": "./dist/tanstack.d.ts",
                "default": "./dist/tanstack.js"
            },
            "require": {
                "types": "./dist/tanstack.d.cts",
                "default": "./dist/tanstack.cjs"
            }
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "keywords": [
        "typescript",
        "react",
        "better-auth",
        "shadcn",
        "nextjs"
    ],
    "author": "daveycodez",
    "license": "MIT",
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@tanstack/react-query": "^5.80.7",
        "@types/node": "^24.0.1",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "better-auth": "^1.3.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "esbuild-plugin-preserve-directives": "^0.0.11",
        "lucide-react": "^0.515.0",
        "react-router": "^7.6.2",
        "tailwind-merge": "^3.3.1",
        "tailwindcss-animate": "^1.0.7",
        "tsc-watch": "^7.1.1",
        "tsup": "^8.5.0",
        "tsx": "^4.20.3",
        "turbo": "^2.5.4",
        "typescript": "^5.8.3"
    },
    "peerDependencies": {
        "@hookform/resolvers": ">=5.0.0",
        "@tanstack/react-query": ">=5.66.0",
        "better-auth": "^1.3.0",
        "class-variance-authority": ">=0.7.0",
        "clsx": ">=2.1.0",
        "input-otp": ">=1.4.0",
        "lucide-react": ">=0.469.0",
        "radix-ui": "^1.4.2",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "react-hook-form": ">=7.55.0",
        "sonner": ">=1.7.0",
        "tailwind-merge": ">=2.6.0",
        "tailwindcss": ">=3.0.0",
        "zod": ">=3.24.0"
    },
    "dependencies": {
        "@better-fetch/fetch": "^1.1.18",
        "@react-email/components": "^0.0.42",
        "js-sha256": "^0.11.1",
        "radix-ui": "^1.4.2",
        "react-qr-code": "^2.0.15",
        "ua-parser-js": "^2.0.3",
        "vaul": "^1.1.2"
    },
    "pnpm": {
        "overrides": {}
    },
    "packageManager": "pnpm@10.12.1"
}
